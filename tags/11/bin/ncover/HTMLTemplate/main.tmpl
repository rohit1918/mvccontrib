    <script language="javascript" type="text/javascript">
    /* <![CDATA[ */
		/* nice script for getting elements by class name, from http://snook.ca/archives/javascript/your_favourite_1/ */
		function getElementsByClassName(node, classname)
		{
			var a = [];
			var re = new RegExp('\\b' + classname + '\\b');
			var els = node.getElementsByTagName("*");
			for(var i=0,j=els.length; i<j; i++)
				if(re.test(els[i].className))a.push(els[i]);
			return a;
		}
    
		/* http://www.quirksmode.org/js/opacity.html */
		function setOpacity(obj, value) {
			obj.style.opacity = value/10;
			obj.style.filter = 'alpha(opacity=' + value*10 + ')';
		}
    
		var updatingWait = false;
    
		function uncoveredEventHandler() {
			updatingWait = true;
			updatingLoop();
			setTimeout(toggleUncovered, 20);
		}
    
		function coveredEventHandler() {
			updatingWait = true;
			updatingLoop();
			setTimeout(toggleCovered, 20);
		}
    
		function updatingLoop() {
			if (updatingWait) {
				showUpdating();
				setTimeout(updatingLoop, 500);
			}
			else {
				showChecks();
			}
		}
    
		function toggleUncovered() {
			var sourceTable = document.getElementById("sourceTable");
			var elements = getElementsByClassName(sourceTable, "uncovered");
			
			if (elements.length > 500) {
				setTimeout(showUpdating, 20);
			}
			
			var color = "#ff9999";
			if (!document.getElementById("uncoveredBox").checked) {
				var color = "transparent";
			}
	
			for (var n = 0; n < elements.length; n++) {
				var currElement = elements[n];
				currElement.style.backgroundColor = color;
			}
			updatingWait = false;
		}
		
		function toggleCovered() {
			var sourceTable = document.getElementById("sourceTable");
			var elements = getElementsByClassName(sourceTable, "covered");
			
			var color = "#5efb6e";
			if (!document.getElementById("coveredBox").checked) {
				var color = "transparent";
			}
	
			for (var n = 0; n < elements.length; n++) {
				var currElement = elements[n];
				currElement.style.backgroundColor = color;
			}
			
			updatingWait = false;
		}
		
		function showUpdating() {
			var toggleChecks = document.getElementById("togglechecks");
			var toggleUpdate = document.getElementById("toggleupdate");
			toggleChecks.style.display = "none";
			toggleUpdate.style.display = "block";
		}
		
		function showChecks() {
			var toggleChecks = document.getElementById("togglechecks");
			var toggleUpdate = document.getElementById("toggleupdate");
			toggleChecks.style.display = "block";
			toggleUpdate.style.display = "none";
		}
	/* ]]> */
    </script>
